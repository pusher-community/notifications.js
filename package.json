{
  "name": "notificationsjs",
  "version": "0.1.0",
  "description": "Coming soon...",
  "main": "lib/notifications.js",
  "scripts": {
    "build-dev": "webpack -w --config webpack.dev.js",
    "example-dev": "live-server --port=8001",
    "build-prod": "rollup --config rollup.config.js && rollup --config rollup.config.js --environment MINIFY",
    "build": "npm run build-prod && cp src/style.css lib",
    "prepublish": "npm run build",
    "test": "npm run lint && ava --tap | tap-dot",
    "test-watch": "ava --watch",
    "lint": "eslint src tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pusher-community/notifications.js.git"
  },
  "keywords": [
    "notifications",
    "pusher",
    "notify"
  ],
  "author": "Jack Franklin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pusher-community/notifications.js/issues"
  },
  "homepage": "https://github.com/pusher-community/notifications.js#readme",
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "0.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "1.1.1",
    "babel-preset-node6": "11.0.0",
    "babel-register": "6.8.0",
    "doubler": "0.6.0",
    "eslint": "2.9.0",
    "jsdom": "8.5.0",
    "live-server": "^1.0.0",
    "object-assign": "4.1.0",
    "rollup": "0.26.2",
    "rollup-plugin-babel": "2.4.0",
    "rollup-plugin-commonjs": "2.2.1",
    "rollup-plugin-node-resolve": "1.5.0",
    "rollup-plugin-uglify": "0.3.1",
    "tap-dot": "1.0.5",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "animate.css": "3.5.1"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "source": [
      "src/*.js"
    ],
    "babel": "inherit",
    "verbose": true,
    "require": [
      "babel-register",
      "./tests/jsdom.js"
    ]
  }
}
